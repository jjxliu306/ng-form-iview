  "private": false,
  "description": "ng-form-iview",
  "main": "lib/ng-form-iview.umd.min.js",
  "directories": {
    "example": "example",
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitee.com/jjxliu306/ng-form-iview.git"
  },
  "author": "liuyufeng",
  "license": "MIT",
  "bugs": {
    "url": "https://gitee.com/jjxliu306/ng-form-iview/issues"
  },
  "homepage": "https://gitee.com/jjxliu306/ng-form-iview#readme",
  "keywords": [
    "ng-form-iview",
    "vue-form-design",
    "form-design-iview",
    "formdesign",
    "动态表单",
    "表单"
  ]
